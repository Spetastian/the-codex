{
  "name": "the-codex",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^4.6.1",
    "material-ui": "^0.19.4",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.14",
    "react-tap-event-plugin": "^3.0.2",
    "redux": "^3.7.2",
    "redux-observable": "^0.17.0",
    "rxjs": "^5.5.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format:check": "prettier -l ./src/**/*.js",
    "format:fix": "prettier --write ./src/**/*.js",
    "lint:check": "eslint ./src",
    "lint:fix": "eslint --fix ./src && npm run format:fix",
    "verify-eslint-rules": "eslint --print-config .eslintrc | eslint-config-prettier-check",
    "ci": "npm run verify-eslint-rules && npm run format:check && npm run lint:check"
  },
  "devDependencies": {
    "eslint-config-prettier": "^2.7.0",
    "prettier": "^1.8.2"
  }
}
